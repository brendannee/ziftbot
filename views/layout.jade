!!! 5
html
  head
    link(rel='shortcut icon', href:'/favicon.ico')
    title= typeof(title) !== 'undefined' ? title : 'ZiftBot'
    script(src='/js/libs/modernizr-2.0.6.min.js')
    link(rel='stylesheet', href='/css/style.css')
    link(rel='stylesheet', href='/css/video-js.css')
  body
    #wrap
      #header
        h1.logo
          a.logo(href='/', title='Ziftbot Home') Ziftbot
        p.lead Find the perfect Zappos gift by answering a few simple questions.
      #content!= body
      footer
        .left By 
          a(href='http://twitter.com/trucy') Trucy Phan
          |  and 
          a(href='http://twitter.com/brendannee') Brendan Nee
          |  and 
          a(href='http://twitter.com/cpetzold') Conner Petzold
          br
          |  using 
          a(href="http://developer.zappos.com/", title="Zappos API")
            img(src="/images/zappos.jpg", alt="Zappos API")
          |  and 
          a(href="http://sendgrid.com/", title="Sendgrid API")
            img(src="/images/sendgrid.jpg", alt="Sendgrid API")
        .right.
          <!-- AddThis Button BEGIN -->
          <div class="addthis_toolbox addthis_default_style ">
          <a class="addthis_button_twitter_follow_native" tf:screen_name="ziftbot" tw:show-count="false" tw:show-screen-name="true" tw:width="65px" style="padding-right:20px" ></a>
          <a class="addthis_button_facebook_like" fb:like:layout="button_count"></a>
          <a class="addthis_button_google_plusone" g:plusone:size="medium"></a>
          </div>
          <script type="text/javascript" src="http://s7.addthis.com/js/250/addthis_widget.js"></script>
          <!-- AddThis Button END -->
          
          
    != partial('templates')
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js')
